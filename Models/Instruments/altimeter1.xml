<!--####    InHg    ####-->

    <animation>
        <type>textranslate</type>
        <object-name>inhg01</object-name>
        <property>instrumentation/altimeter/setting-inhg</property>
        <factor>0.01</factor>
        <step>10</step>
		<bias>0.005</bias>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>inhg02</object-name>
        <property>instrumentation/altimeter/setting-inhg</property>
        <factor>0.1</factor>
