	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>FD1Switch</object-name>
		<property>it-autoflight/input/fd1</property>
		<interpolation>
			<entry><ind>0</ind><dep> 30</dep></entry>
			<entry><ind>1</ind><dep>-30</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.00682</x-m>
			<y-m>0</y-m>
