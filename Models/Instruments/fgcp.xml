	<!-- NAV1 Freq Knobs -->
	<animation>
		<type>rotate</type>
		<object-name>Nav1Knob1</object-name>
		<property>instrumentation/fgcp/nav1-major-rotate</property>
		<factor>-11.25</factor>
		<center>
			<x-m>0</x-m>
			<y-m>-0.25</y-m>
			<z-m>-0.01524</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Nav1Knob1</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable>false</repeatable>
			<binding>
				<condition>
					<property>/systems/electrical/outputs/fgcp-power</property>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/nav[0]/frequencies/selected-mhz</property>
				<step>1</step>
				<min>107.95</min>
				<max>117.95</max>
				<wrap>true</wrap>
			</binding>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/fgcp/nav1-major-rotate</property>
				<step>1</step>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable>false</repeatable>
			<binding>
				<condition>
					<property>/systems/electrical/outputs/fgcp-power</property>
				</condition>
				<command>property-adjust</command>
				<property>instrumentation/nav[0]/frequencies/selected-mhz</property>
				<step>-1</step>
				<min>107.95</min>
				<max>117.95</max>
				<wrap>true</wrap>
			</binding>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/fgcp/nav1-major-rotate</property>
				<step>-1</step>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
