<?xml version="1.0"?>
<PropertyList>

<path>nav_radio.ac</path>

<animation>
  <type>material</type>
  <object-name>Panel</object-name>
  <object-name>TFRbutton</object-name>
  <object-name>StdbyKnob1</object-name>
  <object-name>StdbyKnob2</object-name>
  <object-name>ActKnob1</object-name>
  <object-name>ActKnob2</object-name>
  <emission>
    <red>0.2</red>
    <green>0.1</green>
    <blue>0.05</blue>
    <factor-prop>controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>
<animation>
  <type>material</type>
  <object-name>ActDigit1</object-name>
  <object-name>ActDigit2</object-name>
  <object-name>ActDigit3</object-name>
  <object-name>ActDigit4</object-name>
  <object-name>ActDigit5</object-name>
  <object-name>DecimalActive</object-name>
  <object-name>StdbyDigit1</object-name>
  <object-name>StdbyDigit2</object-name>
  <object-name>StdbyDigit3</object-name>
  <object-name>StdbyDigit4</object-name>
  <object-name>StdbyDigit5</object-name>
  <object-name>DecimalStdby</object-name>
  <emission>
    <red>0.8</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>controls/lighting/digital-norm</factor-prop>
  </emission>
</animation>
	<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>Off</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/bus/dcR</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation> 
<!--  Active Freq Digits -->

<animation>
  <type>textranslate</type>
  <object-name>ActDigit1</object-name>
  <property>instrumentation/nav[1]/frequencies/display-sel-mhz</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
