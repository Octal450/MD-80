<!--  Active Freq Knobs -->

<animation>
  <type>rotate</type>
  <object-name>ActKnob1</object-name>
  <property>instrumentation/nav[1]/frequencies/display-sel-mhz</property>
  <interpolation>
    <entry><ind>107.95</ind><dep>  0</dep></entry>
    <entry><ind>117.95</ind><dep>720</dep></entry>
  </interpolation>
  <center>
    <x-m>0</x-m>
    <y-m>-0.038</y-m>
    <z-m>-0.013</z-m>
  </center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>pick</type>
  <object-name>ActKnob1</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>instrumentation/nav[1]/frequencies/selected-mhz</property>
      <step>1</step>
      <min>107.95</min>
      <max>117.95</max>
      <wrap>true</wrap>
    </binding>
  </action>
</animation>
<animation>
  <type>pick</type>
  <object-name>ActKnob1</object-name>
  <action>
    <button>1</button>
    <repeatable>false</repeatable>
    <binding>
