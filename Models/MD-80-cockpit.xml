		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>PanelMainC3E</object-name>
		<condition>
			<property>systems/acconfig/options/edp-sdp</property>
		</condition>
	</animation>
	
	<!-- Model Imports -->
	<model>
		<name>Overhead</name>
		<path>Aircraft/MD-80/Models/Overhead/overhead.xml</path>
		<offsets>
			<x-m>3.05128</x-m>
			<y-m>0.0</y-m>
			<z-m>1.66523</z-m>
			<pitch-deg>63</pitch-deg>
		</offsets>
	</model>
	
	<!-- No lights without power! -->
	<animation>
		<type>select</type>
		<object-name>Slat.Auto</object-name>
		<object-name>Slat.Disag</object-name>
		<object-name>Slat.Land</object-name>
		<object-name>Slat.Takeoff</object-name>
		<condition>
			<greater-than-equals>
				<property>systems/electrical/outputs/efis</property>
				<value>25</value>
			</greater-than-equals>
		</condition>
	</animation>
	
	<!-- Lights -->
