<?xml version="1.0"?>

<PropertyList>
	
	<!-- Sound View Logic -->
	<logic>
		<input>
			<or>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>15</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>16</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>17</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>18</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>19</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>20</value>
				</equals>
			</or>
		</input>
		<output>/sim/sound/wingview</output>
	</logic>
	
	<logic>
		<input>
			<or>
				<equals>
					<property>/sim/current-view/internal</property>
					<value>1</value>
				</equals>
				<property>/sim/sound/wingview</property>
			</or>
		</input>
		<output>/sim/sound/internal-custom</output>
	</logic>
	
	<logic>
		<input>
			<not-equals>
				<property>/sim/sound/internal-custom</property>
				<value>1</value>
			</not-equals>
		</input>
		<output>/sim/sound/external-custom</output>
	</logic>
	
	<!-- Fake N2 because numbers pulled from SASA with permission - make sure it matches engine values -->
	<filter>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<less-than>
					<property>/engines/engine[0]/n2-actual</property>
					<value>54.199</value>
				</less-than>
			</condition>
			<property>/engines/engine[0]/n2-actual</property>
		</input>
		<input>
			<expression>
				<table>
					<property>/engines/engine[0]/n1-actual</property>
					<entry><ind>  0.0</ind><dep>  0.0</dep></entry>
					<entry><ind> 25.3</ind><dep> 54.2</dep></entry>
					<entry><ind>101.6</ind><dep>102.5</dep></entry>
				</table>
			</expression>
		</input>
		<output>/sim/sound/fake-n2[0]</output>
	</filter>
	
	<filter>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<less-than>
					<property>/engines/engine[1]/n2-actual</property>
					<value>54.199</value>
				</less-than>
			</condition>
			<property>/engines/engine[1]/n2-actual</property>
		</input>
		<input>
			<expression>
				<table>
					<property>/engines/engine[1]/n1-actual</property>
					<entry><ind>  0.0</ind><dep>  0.0</dep></entry>
					<entry><ind> 25.3</ind><dep> 54.2</dep></entry>
					<entry><ind>101.6</ind><dep>102.5</dep></entry>
				</table>
			</expression>
		</input>
		<output>/sim/sound/fake-n2[1]</output>
	</filter>
	
	<filter>
		<type>noise-spike</type>
		<input>
			<condition>
				<and>
					<greater-than-equals>
						<property>/systems/electrical/outputs/efis</property>
						<value>24</value>
					</greater-than-equals>
					<equals>
						<property>/gear/gear[0]/wow</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/controls/pneumatics/radio-rack</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
			<value>2</value>
		</input>
		<input>0</input>
		<output>/sim/sound/avionics-fanp</output>
		<max-rate-of-change>2.0</max-rate-of-change>
	</filter>
	
	<filter>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<table>
					<property>/sim/sound/avionics-fanp</property>
					<entry><ind>0.0</ind><dep>0</dep></entry>
					<entry><ind>0.3</ind><dep>1</dep></entry>
				</table>
			</expression>
		</input>
		<input>0</input>
		<output>/sim/sound/avionics-fan</output>
	</filter>
	
	<!-- Cockpit Noise -->
	<filter>
		<type>gain</type>
		<gain>1</gain>
		<input>
			<expression>
				<product>
					<table>
						<property>/velocities/groundspeed-kt</property>
						<entry><ind>  1</ind><dep>0</dep></entry>
						<entry><ind>110</ind><dep>1</dep></entry>
						<entry><ind>130</ind><dep>2</dep></entry>
					</table>
				</product>
			</expression>
		</input>
		<output>/sim/sound/other/roll</output>
	</filter>
	
	<!-- Wind -->
	<filter>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<property>/sim/sound/internal-custom</property>
			</condition>
			<expression>
				<table>
					<property>/velocities/airspeed-kt</property>
					<entry><ind>100</ind><dep>0.0</dep></entry>
					<entry><ind>198</ind><dep>0.1</dep></entry>
					<entry><ind>250</ind><dep>0.5</dep></entry>
				</table>
			</expression> 
		</input>
		<input>0</input>
		<output>/sim/sound/wind-v</output>
	</filter>
	
