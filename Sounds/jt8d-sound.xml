<?xml version="1.0"?>
 
<PropertyList>
	<fx>
		
		<apoff>
			<name>apoff</name>
			<path>Aircraft/MD-80/Sounds/apoff.wav</path>
			<condition>
				<property>/it-autoflight/sound/apoffsound</property>
			</condition>
			<volume>
				<factor>10.0</factor>
			</volume>
		</apoff>
		
		<wow>
			<mode>once</mode>
			<name>wow0</name>
			<path>Aircraft/MD-80/Sounds/Touch-Main.wav</path> <!-- Nose seemed to harsh -->
			<condition>
				<property>gear/gear[0]/wow</property>
			</condition>
		</wow>
		
		<wow>
			<mode>once</mode>
			<name>wow1</name>
			<path>Aircraft/MD-80/Sounds/Touch-Main.wav</path>
			<condition>
				<property>gear/gear[1]/wow</property>
			</condition>
		</wow>
		
		<wow>
			<mode>once</mode>
			<name>wow2</name>
			<path>Aircraft/MD-80/Sounds/Touch-Main.wav</path>
			<condition>
				<property>gear/gear[2]/wow</property>
			</condition>
		</wow>
		
		<!-- AVIONICS -->
	
		<MDAMB>
			<name>MD EFIS</name>
			<path>Aircraft/MD-80/Sounds/SASA/avionics-rattle.wav</path>
			<mode>looped</mode>
			<condition>
				<!--<or>
					<greater-than>
						<property>/systems/electrical/bus/dc-l</property>
						<value>20</value>
					</greater-than>
					<greater-than>
						<property>/systems/electrical/bus/dc-r</property>
						<value>20</value>
					</greater-than>
				</or>-->
				<greater-than>
					<property>/systems/electrical/outputs/avionics-fan</property>
					<value>20</value>
				</greater-than>
			</condition>
			<property>sim/current-view/internal</property>
			<volume>
				<property>MD80/ambience/cockpit-amb-z</property>
			</volume>
		</MDAMB>
		
		<!-- CABIN NOISE -->
		
		<MDAMB>
			<name>MD NOISE</name>
			<path>Aircraft/MD-80/Sounds/SASA/cabin-fans.wav</path>
