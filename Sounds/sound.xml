<PropertyList>
	
	<fx>
		
		<sb>
			<mode>once</mode>
			<name>sb</name>
			<path>Aircraft/MD-80/Sounds/Cockpit/seatbelt.wav</path>
			<condition>
				<and>
					<greater-than-equals>
						<property>/systems/electrical/outputs/efis</property>
						<value>24</value>
					</greater-than-equals>
					<property>/sim/sound/seatbelt-sign</property>
				</and>
			</condition>
			<volume>
				<property>/sim/sound/internal-custom</property>
			</volume>
		</sb>
		
