<?xml version="1.0"?>

<PropertyList>
	
	<!-- Sound View Logic -->
	<logic>
		<input>
			<or>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>15</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>16</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>17</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>18</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>19</value>
				</equals>
				<equals>
					<property>/sim/current-view/view-number</property>
					<value>20</value>
				</equals>
			</or>
		</input>
		<output>/sim/sound/wingview</output>
	</logic>

	<logic>
		<input>
			<or>
				<equals>
					<property>/sim/current-view/internal</property>
					<value>1</value>
				</equals>
				<property>/sim/sound/wingview</property>
			</or>
		</input>
		<output>/sim/sound/internal-custom</output>
	</logic>

	<logic>
		<input>
			<not-equals>
				<property>/sim/sound/internal-custom</property>
				<value>1</value>
			</not-equals>
		</input>
		<output>/sim/sound/external-custom</output>
	</logic>
	
	<filter>
		<type>noise-spike</type>
		<input>
			<condition>
				<and>
					<greater-than-equals>
						<property>/systems/electrical/outputs/efis</property>
						<value>24</value>
					</greater-than-equals>
					<or>
						<equals>
							<property>/gear/gear[0]/wow</property>
							<value>1</value>
						</equals>
						<equals>
							<property>/controls/pneumatics/switches/radio-rack</property>
							<value>1</value>
						</equals>
					</or>
				</and>
			</condition>
			<value>2.0</value>
		</input>
		<input>0</input>
		<output>/sim/sound/avionics-fanp</output>
		<max-rate-of-change>2.0</max-rate-of-change>
	</filter>
	
	<filter>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<expression>
				<table>
					<property>/sim/sound/avionics-fanp</property>
					<entry><ind>0.0</ind><dep>0</dep></entry>
					<entry><ind>0.3</ind><dep>1</dep></entry>
				</table>
			</expression>
		</input>
		<input>0</input>
		<output>/sim/sound/avionics-fan</output>
	</filter>
	
