					26  1.07692307692
					27  1.03703703703
					28  1.00000000000
					29  0.96551724137
					30  0.93333333333
					31  0.90322580645
					32  0.87500000000
					33  0.84848484848
				</tableData>
			</table>
		</scheduled_gain>
		
		<!-- Emergency Power Arm Logic -->
		<switch name="/systems/electrical/relay/emer-pwr/contact-pos">
			<default value="0"/>
			<test logic="AND" value="1">
				/controls/electrical/emer-pwr eq 1
				/systems/electrical/bus/dc-bat ge 24
			</test>
		</switch>
		
		<!-- Static Inverter -->
		<fcs_function name="/systems/electrical/sources/si/output-volt">
			<function>
				<product>
					<table> <!-- DC to AC converter -->
						<independentVar lookup="row">/systems/electrical/bus/dc-bat</independentVar>
						<independentVar lookup="column">/systems/electrical/relay/emer-pwr/contact-pos</independentVar>
						<tableData>
							    0    1
							 0  0    0
							24  0  112
							28  0  115
							29  0  118
						</tableData>
					</table>
					<table>
						<independentVar lookup="row">/systems/failures/electrical/si</independentVar>
						<tableData>
							0  1
							1  0
						</tableData>
					</table>
				</product>
			</function>
		</fcs_function>
		
		<!-- Emergency AC Bus Relays -->
		<switch name="/systems/electrical/relay/ac-src-emer-ac/contact-pos">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/bus/ac-l lt 112
				/systems/electrical/bus/ac-r ge 112
			</test>
		</switch>
		
		<actuator name="/systems/electrical/relay/ac-src-emer-ac/contact-final">
			<input>/systems/electrical/relay/ac-src-emer-ac/contact-pos</input>
			<rate_limit>4.8</rate_limit>
		</actuator>
		
		<switch name="/systems/electrical/relay/ac-src-emer-ac/output">
			<default value="0"/>
			<test value="/systems/electrical/bus/ac-l">
				/systems/electrical/relay/ac-src-emer-ac/contact-final eq 0
			</test>
			<test value="/systems/electrical/bus/ac-r">
				/systems/electrical/relay/ac-src-emer-ac/contact-final eq 1
			</test>
		</switch>
		
		<actuator name="/systems/electrical/relay/ac-inv-src-emer-ac/contact-final">
			<input>/systems/electrical/relay/emer-pwr/contact-pos</input>
			<rate_limit>4.8</rate_limit>
		</actuator>
		
		<switch name="/systems/electrical/relay/ac-inv-src-emer-ac/output">
			<default value="0"/>
			<test value="/systems/electrical/relay/ac-src-emer-ac/output">
				/systems/electrical/relay/ac-inv-src-emer-ac/contact-final eq 0
			</test>
			<test value="/systems/electrical/sources/si/output-volt">
				/systems/electrical/relay/ac-inv-src-emer-ac/contact-final eq 1
			</test>
		</switch>
		
		<!-- Emergency AC Bus -->
		<pure_gain name="/systems/electrical/bus/emer-ac">
			<input>/systems/electrical/relay/ac-inv-src-emer-ac/output</input>
			<gain>1.0</gain>
		</pure_gain>
		
		<!-- Emergency DC Bus Relays -->
		<switch name="/systems/electrical/relay/dc-src-emer-dc/contact-pos">
			<default value="0"/>
			<test value="1"> <!-- It follows the Emergency AC Bus -->
				/systems/electrical/relay/ac-src-emer-ac/contact-pos eq 1
			</test>
		</switch>
		
		<switch name="/systems/electrical/relay/dc-src-emer-dc/output">
			<default value="0"/>
			<test value="/systems/electrical/bus/dc-l">
				/systems/electrical/relay/dc-src-emer-dc/contact-pos eq 0
			</test>
			<test value="/systems/electrical/bus/dc-r">
				/systems/electrical/relay/dc-src-emer-dc/contact-pos eq 1
			</test>
		</switch>
		
		<switch name="/systems/electrical/relay/dc-bat-src-emer-dc/contact-pos">
			<default value="0"/>
			<test value="1">
				/systems/electrical/relay/emer-pwr/contact-pos eq 1
			</test>
		</switch>
		
		<switch name="/systems/electrical/relay/dc-bat-src-emer-dc/output">
			<default value="/systems/electrical/relay/dc-src-emer-dc/output"/>
			<test value="/systems/electrical/bus/dc-bat">
				/systems/electrical/relay/dc-bat-src-emer-dc/contact-pos eq 1
			</test>
		</switch>
		
		<!-- Emergency DC Bus -->
		<pure_gain name="/systems/electrical/bus/emer-dc">
			<input>/systems/electrical/relay/dc-bat-src-emer-dc/output</input>
			<gain>1.0</gain>
		</pure_gain>
		
		<!-- Battery Charger and Transfer Relays -->
		<switch name="/systems/electrical/sources/bat-charger-powered">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/relay/emer-pwr/contact-pos ne 1
				/systems/electrical/bus/ac-gndsvc ge 112
			</test>
		</switch>
		
		<switch name="/systems/electrical/relay/dc-bat-trans/contact-pos">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/relay/gndsvc-src-ac-gndsvc/contact-pos eq 0
				/systems/electrical/bus/dc-l lt 24
				/systems/electrical/bus/dc-r lt 24
				/systems/electrical/bus/dc-bat ge 24
			</test>
		</switch>
		
		<switch name="/systems/electrical/relay/dc-bat-trans/output">
			<default value="0"/>
			<test value="/systems/electrical/bus/dc-bat">
				/systems/electrical/relay/dc-bat-trans/contact-pos eq 1
			</test>
		</switch>
		
		<!-- DC Transfer Bus -->
		<fcs_function name="/systems/electrical/bus/dc-trans">
			<function>
				<max>
					<property>/systems/electrical/relay/dc-bat-src-emer-dc/output</property>
					<property>/systems/electrical/relay/dc-bat-trans/output</property>
					<property>/systems/electrical/relay/tr-r2-dc/output-dc-trans</property>
				</max>
			</function>
		</fcs_function>
		
		<!-- Power Instrumentation -->
		<switch name="/systems/electrical/generic/ac-l">
			<default value="0"/>
			<test value="115">
				/systems/electrical/bus/ac-l ge 112
			</test>
			<output>/systems/electrical/outputs/firetank</output>
		</switch>
		
		<switch name="/systems/electrical/generic/ac-radio-l">
			<default value="0"/>
			<test value="28">
				/systems/electrical/bus/ac-radio-l ge 112
			</test>
			<output>/systems/electrical/outputs/adf[0]</output>
			<output>/systems/electrical/outputs/ra[0]</output>
		</switch>
		
		<switch name="/systems/electrical/generic/ac-radio-r">
			<default value="0"/>
			<test value="28">
				/systems/electrical/bus/ac-radio-r ge 112
			</test>
			<output>/systems/electrical/outputs/adf[1]</output>
			<output>/systems/electrical/outputs/ra[1]</output>
		</switch>
		
		<switch name="/systems/electrical/generic/dc-radio-l">
			<default value="0"/>
			<test value="28">
				/systems/electrical/bus/dc-radio-l ge 24
			</test>
			<output>/systems/electrical/outputs/comm[1]</output>
			<output>/systems/electrical/outputs/fma[0]</output>
		</switch>
		
		<switch name="/systems/electrical/generic/dc-radio-both">
			<default value="0"/>
			<test logic="OR" value="28">
				/systems/electrical/bus/dc-radio-l ge 24
				/systems/electrical/bus/dc-radio-r ge 24
			</test>
			<output>/systems/electrical/outputs/transponder</output>
		</switch>
		
		<switch name="/systems/electrical/generic/dc-radio-r">
			<default value="0"/>
			<test value="28">
				/systems/electrical/bus/dc-radio-r ge 24
			</test>
			<output>/systems/electrical/outputs/comm[2]</output>
			<output>/systems/electrical/outputs/fma[1]</output>
			<output>/systems/electrical/outputs/nav[1]</output>
		</switch>
		
		<switch name="/systems/electrical/generic/emer-dc">
			<default value="0"/>
			<test value="28">
				/systems/electrical/bus/emer-dc ge 24
			</test>
			<output>/systems/electrical/outputs/comm[0]</output>
			<output>/systems/electrical/outputs/nav[0]</output>
		</switch>
		
		<!-- Output to Generics to keep FG things working, temporary, until we know what gets power by what bus -->
		<switch name="/systems/electrical/generic/pwr">
			<description>This makes the FG stuff work because we killed the generic electrical</description>
			<default value="0"/>
			<test logic="OR" value="28">
				/systems/electrical/bus/dc-radio-l ge 24
				/systems/electrical/bus/dc-radio-r ge 24
				/systems/electrical/bus/emer-dc ge 24
			</test>
			<output>/systems/electrical/outputs/efis</output>
			<output>/systems/electrical/outputs/fgcp</output>
			<output>/systems/electrical/outputs/gps</output>
			<output>/systems/electrical/outputs/idk</output> <!-- Power stuff that I don't know what powers it -->
			<output>/systems/electrical/outputs/mk-viii</output>
			<output>/systems/electrical/outputs/turn-coordinator</output>
		</switch>
	
	</channel>
	
	<channel name="Overhead Lights">
		
		<switch name="/systems/electrical/lights/emer-pwr-on">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/bus/dc-bat ge 24
				/systems/electrical/relay/emer-pwr/contact-pos eq 1
			</test>
		</switch>
		
		<switch name="/systems/electrical/lights/apu-bus-l">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/bus/dc-bat ge 24
				<test logic="OR">
					/systems/electrical/relay/apu-ac-gen-l/contact-final eq 1
					/controls/switches/annun-test eq 1
				</test>
			</test>
		</switch>
		
		<switch name="/systems/electrical/lights/apu-bus-r">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/bus/dc-bat ge 24
				<test logic="OR">
					/systems/electrical/relay/apu-ac-gen-r/contact-final eq 1
					/controls/switches/annun-test eq 1
				</test>
			</test>
		</switch>
		
		<switch name="/systems/electrical/lights/ext-bus-l">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/bus/dc-bat ge 24
				<test logic="OR">
					/systems/electrical/relay/ext-ac-gen-l/contact-final eq 1
					/controls/switches/annun-test eq 1
				</test>
			</test>
		</switch>
		
		<switch name="/systems/electrical/lights/ext-bus-r">
			<default value="0"/>
			<test logic="AND" value="1">
				/systems/electrical/bus/dc-bat ge 24
				<test logic="OR">
					/systems/electrical/relay/ext-ac-gen-r/contact-final eq 1
					/controls/switches/annun-test eq 1
				</test>
			</test>
		</switch>
	
	</channel>

</system>
