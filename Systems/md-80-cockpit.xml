				/controls/lighting/thunderstorm eq 1
			</test>
			<test value="/controls/lighting/capt-digital">
				/systems/electrical/bus/ac-l ge 112
			</test>
		</switch>
		
		<switch name="/systems/lighting/capt-panel-cmd">
			<default value="0"/>
			<test value="/controls/lighting/capt-panel">
